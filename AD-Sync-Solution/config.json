    "ScriptRoot": "C:\\Scripts\\ADSync",
    "LogPath": "C:\\Scripts\\ADSync\\Logs",
    "CredentialFile": "C:\\Scripts\\ADSync\\encrypt.txt"
  },
  "SafetyThresholds": {
    "DeletionThreshold": 45,
    "AdditionThreshold": 300,
    "UpdateThreshold": 300
